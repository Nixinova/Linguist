{
	"name": "linguist-js",
	"version": "2.1.2",
	"description": "Analyse languages used in a folder. Powered by GitHub Linguist, although it doesn't need to be installed.",
	"main": "dist/index.js",
	"bin": {
		"linguist": "bin/index.js"
	},
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"prepublish": "tsc",
		"prepare": "npm test && npm run perf",
		"perf": "tsc && node test/perf",
		"test": "tsc && node test/test"
	},
	"files": [
		"bin/",
		"dist/**/*.js",
		"dist/*.d.ts"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Nixinova/Linguist.git"
	},
	"keywords": [
		"linguist",
		"languages",
		"language-analysis",
		"language-analyzer"
	],
	"author": "Nixinova (https://nixinova.com)",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Nixinova/Linguist/issues"
	},
	"homepage": "https://github.com/Nixinova/Linguist#readme",
	"dependencies": {
		"binary-extensions": "^2.2.0",
		"classificator": "~0.3.4",
		"commander": "^8.2.0",
		"cross-fetch": "^3.1.4",
		"glob-to-regexp": "~0.4.1",
		"isbinaryfile": "^4.0.8",
		"js-yaml": "^4.1.0",
		"node-cache": "^5.1.2"
	},
	"devDependencies": {
		"@types/classificator": "ts4.4",
		"@types/glob-to-regexp": "ts4.4",
		"@types/js-yaml": "ts4.4",
		"@types/node": "ts4.4",
		"deep-object-diff": "^1.1.0",
		"typescript": "~4.4.3"
	}
}
